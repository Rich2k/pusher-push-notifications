{
    "name": "rich2k/pusher-push-notifications",
    "description": "Pusher native Push Notifications driver.",
    "keywords": [
        "pusher-push-notifications",
        "laravel",
        "pusher",
        "notifications"
    ],
    "homepage": "https://github.com/clementblanco/pusher-push-notifications",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Said",
            "email": "themsaid@gmail.com",
            "homepage": "https://themsaid.com"
        },
        {
            "name": "Marcel Pociot",
            "email": "hello@marcelpociot.com",
            "homepage": "http://marcelpociot.com"
        },
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be"
        },
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://sebastiandedeyne.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/events": "5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/notifications": "5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/queue": "5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "illuminate/support": "5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.* || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "pusher/pusher-php-server": "3.*||4.*||5.*||6.*||7.*"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\PusherPushNotifications\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\PusherPushNotifications\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NotificationChannels\\PusherPushNotifications\\PusherPushNotificationsServiceProvider"
            ]
        }
    }
}
